package cs320.finalexam;

public class Item {
	
	String name;
	double price;
	String description;

	public String getName() {
		return name;
	}
	
	public double getPrice() {
		return price;
	}
	public void setPrice(double price) {
		this.price = price;
	}
	
	public String getDescription() {
		return description;
	}	
}
